<?xml version='1.0' encoding="UTF-8"?>
<jtlshopplugin>
    <Name>JtlShopPluginStarterKit</Name>
    <Description> starter kite inspired by Laravel framework and mvc pattern and it's wrap for Jtlshop built in classes</Description>
    <Author>Omar Hossam El-Din Kandil</Author>
    <URL>https://www.linkedin.com/in/omar-hossameldin-kandil-74633a1bb/</URL>
    <PluginID>JtlShopPluginStarterKit</PluginID>
    <XMLVersion>100</XMLVersion>
    <ShopVersion>5.0.0</ShopVersion>
    <Version>1.0.0</Version>
    <CreateDate>2021-09-23</CreateDate>
    <Install>
        <FlushTags>CACHING_GROUP_CATEGORY, CACHING_GROUP_ARTICLE</FlushTags>
        <Adminmenu>
            <Customlink sort="1">
                <Name>Api_Credentials</Name>
            </Customlink>
            <Customlink sort="2">
                <Name>Email_Credentials</Name>
            </Customlink>
            <Customlink sort="3">
                <Name>Test_Email_Credentials</Name>
            </Customlink>
            <Customlink sort="4">
                <Name>All_Posts</Name>
            </Customlink>
            <Customlink sort="5">
                <Name>All_Categories</Name>
            </Customlink>
        </Adminmenu>
        <FrontendLink>
            <Link>
                <Name>point_router</Name>
                <Template>point_router.tpl</Template>                
                <LinkLanguage iso="GER">
                    <Seo>ressource</Seo>
                    <Name>ressource</Name>
                    <Title>ressource</Title>
                    <MetaTitle>ressource</MetaTitle>
                    <MetaKeywords>ressource</MetaKeywords>
                    <MetaDescription>ressource</MetaDescription>
                </LinkLanguage>
            </Link>
        </FrontendLink>
        <JS>
            <file>
                <name>HttpRequest.js</name>
                <priority>1</priority>
                <position>body</position>
            </file>
            <file>
                <name>main.js</name>
                <priority>1</priority>
                <position>body</position>
            </file>
        </JS>
    </Install>
</jtlshopplugin>
